{{!< default}}
{{#post}}

	<!-- Map -->
	<iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3151.332151421229!2d-122.25822638411537!3d37.82910927974961!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x80857dfa59823ee1%3A0xb363ce29fc957413!2s4129+Broadway%2C+Oakland%2C+CA+94611%2C+USA!5e0!3m2!1sen!2sgr!4v1451209654308" width="100%" height="450" frameborder="0" style="border:0" allowfullscreen></iframe>
	<!-- End of Map -->

	{{> "share-buttons"}}

<div class="articles-container single">
	<div class="row">
		<div class="col-sm-12">

<div class="block-grid-xs-1">
			<div class="block-grid-item">

				<article class="{{post_class}} single">

					<div class="article-content single">
						<div class="post-excerpt">
							<p>{{content}}</p>

							{{#if tags}}
							<div class="post-tags">
								<span class="tag">{{tags separator=" "}}</span>
							</div>
							 {{/if}}

						</div>
					</div>

				</article>

			</div>
</div>
<div class="clearance"></div>

</div>
</div>
</div>

<div class="spacing-35">
</div>

{{/post}}
